<% provide(:title, "OfficeSignup") %>
<h1>拠点登録</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
  　
    <%= form_with(model: @office, local: true) do |f| %>
    <%= render "shared/error_messages", object: f.object %>
    
      <%= f.label :office_name, class: "label-offices" %>
      <%= f.text_field :office_name, class: "form-control" %>
      
      <%= f.label :office_number, class: "label-offices" %>
      <%= f.text_field :office_number, class: "form-control" %>
      
      <%= f.submit "登録", class: "btn btn-primary btn-block btn-offices" %>
    <% end %>
  </div>
</div>

